    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "parcel": "^2.3.2",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5",
    "web-ext": "^6.7.0",
    "web-ext-types": "^3.2.1"
  },
  "dependencies": {
    "uuid": "^8.3.2"
  }
